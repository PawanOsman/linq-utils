{
	"name": "linq-utils",
	"version": "1.1.1",
	"description": "A lightweight and easy-to-use library that provides LINQ-like query capabilities for JavaScript arrays.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
	  ".": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs"
	  }
	},
	"files": [
	  "/dist"
	],
	"scripts": {
		"start": "tsup; node examples/index.js",
		"build": "tsup"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PawanOsman/linq-utils"
	},
	"keywords": [
		"querying",
		"filtering",
		"worting",
		"projection",
		"joining",
		"grouping",
		"aggregation",
		"extension methods",
		"ienumerable interface",
		".net",
		"c#",
		"sql-like"
	],
	"author": "PawanOsman",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "18.11.18",
		"tsup": "^6.6.3"
	},
	"bugs": {
		"url": "https://github.com/PawanOsman/linq-utils/issues"
	},
	"homepage": "https://github.com/PawanOsman/linq-utils#readme"
}
